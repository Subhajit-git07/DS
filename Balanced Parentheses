class stack:
    def __init__(self):
        self.items = []
        
    def push(self,item):
        self.items.append(item)
        
    def pop(self):
        return self.items.pop()
    
    def is_empty(self):
        return self.items == []
    
    def size(self):
        return len(self.items)
    
    def peek(self):
        return self.items[len(self.items)-1]
    
    def print_stack(self):
        print(self.items)
        
def parchecker(symbolString):
    s = stack()
    match = True
    for i in symbolString:
        if i == "(":
            s.push(i)
        else:
            if s.is_empty():
                match = False
            else:
                s.pop()
    if match and s.is_empty():
        return True
    else:
        return False
parchecker("(())")   
